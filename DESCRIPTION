Package: startR
Title: Automatically Retrieve Multidimensional Distributed Data Sets
Version: 2.3.0
Authors@R: c(
    person("Nicolau", "Manubens", , "nicolau.manubens@bsc.es", role = c("aut")),
    person("An-Chi", "Ho", , "an.ho@bsc.es", role = c("aut", "cre")), 
